vector<vector<int>> insert(vector<vector<int>>& arr, vector<int>& nums) {
        int n=arr.size();
        vector<vector<int>> ans;
        arr.push_back(nums);
        sort(arr.begin(),arr.end());

        if(arr.size()<=1) return arr;
        
        int start=arr[0][0];
        int end=arr[0][1];

        for(int i=1;i<n+1;i++){
            if(arr[i][0] <= end){
                end=max(arr[i][1],end);
            }
            else{
                ans.push_back({start,end});
                start=arr[i][0];
                end=arr[i][1];
            }
        }
        ans.push_back({start,end});
        return ans;
}
